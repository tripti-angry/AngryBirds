package io.github.some_example_name;

import com.badlogic.gdx.Game;
import com.badlogic.gdx.graphics.g2d.SpriteBatch;

public class AngryBirds extends Game { // This class should be named AngryBirds
    public SpriteBatch batch; // Make batch public if needed in MainMenuScreen

    @Override
    public void create() {
        batch = new SpriteBatch();
        setScreen(new MainMenuScreen(this)); // Pass the AngryBirds instance to MainMenuScreen
    }

    @Override
    public void dispose() {
        batch.dispose();
        super.dispose(); // Call the superclass dispose method
    }
}
